{
  "name": "directus",
  "private": true,
  "version": "1.0.0",
  "description": "Directus admin",
  "dependencies": {
    "directus": "^10.7.2",
    "pg": "^8.11.3",
    "endpoint-example": "workspace:*"
  },
  "devDependencies": {
    "create-directus-extension": "^10.0.15"
  },
  "scripts": {
    "start": "directus start",
    "start:directus": "directus start",
    "init-config": "cp -n example.env .env || true",
    "preinstall": "pnpm init-config",
    "dev": "NODE_OPTIONS='--inspect' NODE_TLS_REJECT_UNAUTHORIZED='0' directus start",
    "postinstall": "directus bootstrap && directus database migrate:latest",
    "extension": "create-directus-extension"
  },
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "~8.10.2"
  }
}
